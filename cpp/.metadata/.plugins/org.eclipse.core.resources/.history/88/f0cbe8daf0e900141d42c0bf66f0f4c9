#include "Timer.h"


Timer::Timer(int id) : id_(id) {}

int Timer::handle_timeout (const ACE_Time_Value &tv,const void *arg)
{
	ACE_TRACE (ACE_TEXT ("Timer::handle_timeout"));

	const int *val = static_cast<const int*>(arg);
	ACE_ASSERT((*val) == id_);
	ACE_DEBUG ((LM_DEBUG,
				ACE_TEXT ("Expiry handled by thread %t\n")));
	return 0;
}
